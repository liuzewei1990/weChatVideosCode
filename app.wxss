/**app.wxss**/

page{
  background: #f2f2f2;
}


.martop20{
  margin-top: 20rpx;
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.x-btn{
  background-color: rgb(20, 138, 167);
  color: #FFF;
  width: 95%;
  margin: 100rpx auto;
}

.clear::after{
  content: ".";
  display: block;
  height: 0;
  clear: both;
  visibility: hidden;
}

.fixed{
  position: fixed;
  left: 0;
  top: 0;
  z-index: 99;
  width: 100%;
}